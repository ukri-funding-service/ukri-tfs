{
    "ignore": ["UI/config"],
    "presets": [
        [
            "@babel/preset-env",
            {
                "targets": {
                    "node": "current"
                }
            }
        ]
    ],
    "plugins": [
        "@babel/plugin-transform-runtime",
        "@babel/plugin-transform-class-properties",
        "@babel/plugin-proposal-export-default-from"
    ]
}
