{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://funding-service.ukri.org/schema/award/event/v1/returnedForAmendmentsData.schema.json",
    "title": "Returned For Amendments",
    "description": "Data in the event emitted when an internal user has returned an award for amendments",
    "type": "object",
    "required": ["awardReference", "submittedBy", "timeSubmitted"],
    "additionalProperties": false,
    "properties": {
        "awardReference": {
            "$ref": "returnedAwardProperties/awardReference.schema.json"
        },
        "submittedBy": {
            "$ref": "returnedAwardProperties/submittedBy.schema.json"
        },
        "timeSubmitted": {
            "$ref": "returnedAwardProperties/timeSubmitted.schema.json"
        }
    }
}
