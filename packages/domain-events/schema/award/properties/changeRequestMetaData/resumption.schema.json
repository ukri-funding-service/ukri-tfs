{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://funding-service.ukri.org/schema/award/event/properties/changeRequestMetaData/resumption.schema.json",
    "title": "CR Submitted - Resumption",
    "description": "Data in the event emitted when a change request is submitted specific to a resumption request",
    "type": "object",
    "properties": {
        "changeType": {
            "const": "Resumption"
        },
        "requestedResumptionDate": {
            "description": "The requested date for resumption.",
            "type": "string",
            "format": "date-time"
        }
    },
    "required": ["changeType", "requestedResumptionDate"]
}
